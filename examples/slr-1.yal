let delim = [' ''\t''\n']
let ws = delim+
let letter = ['A'-'Z''a'-'z']
let digit = ['0'-'9']
let id = (letter)(letter|digit)*
let plus = '+'
let times = '*'
let lparen = '('  
let rparen = ')'
let NUMBER = 'a'

rule tokens = 
  | ws      { WS }  
  | ID     { ID }   
  | PLUS      
  | TIMES       
  | LPAREN
  | RPAREN     
  | NUMBER

